import java.util.*;
public class Main
{
	public static void main(String[] args) {
		System.out.println("Enter year");
		Scanner sc= new Scanner (System.in);
		int y=sc.nextInt();
		if (y%4==0)
		{
		    System.out.println("leap year");
		}
		else 
		{
		    System.out.println(" not leap year");
		}
	}
}
