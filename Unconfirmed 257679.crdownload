import java.util.*;
 public class Main
{
    public static void main (String[] args)
    {
        System.out.println("Enter number ");
        Scanner sc=new Scanner (System.in);
        int n = sc.nextInt();
        int sum=0,i;
        for(i=1;i<=n;i++)
        {
            sum=sum+i;
        }
        System.out.println("sum"  +   sum);
    }
}